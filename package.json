    "@types/babel__core": "^7.1.20",
    "@types/eslint": "^8.4.10",
    "@types/fs-extra": "^9.0.13",
    "@types/graphql-fields": "^1.3.4",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cspell": "^6.14.3",
    "eslint": "^8.28.0",
    "eslint-config-alloy": "^4.7.0",
    "eslint-plugin-spellcheck": "^0.0.19",
    "jest": "^29.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "lockfile-lint": "^4.9.6",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.1",
    "typescript": "~4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "resolutions": {
    "@keycloak/keycloak-admin-client": "^19.0.3",
    "axios": "~0.27.2",
    "pino": "^8.7.0"
  },
  "transpileModules": [
    "@keycloak/keycloak-admin-client",
    "@risserlabs/nestjs-keycloak",
    "camelize-ts",
    "chalk",
    "lodash-es",
    "url-join",
    "url-template"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "eslintIgnore": [
    "dist",
    "src/generated"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/es/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/_setup.ts"
    ]
  },
  "jestSonar": {
    "reportPath": ".mkpm/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "docker": {
    "registry": "registry.gitlab.com/risserlabs/community"
  },
  "packageManager": "yarn@3.2.0",
  "prettier": "eslint-config-alloy/.prettierrc.js",
  "nodemonConfig": {
    "watch": [
      "src",
      "model",
      "package.json",
      "tsconfig.json",
      ".env"
    ],
    "ext": "ts,tsx,js,jsx,mjs,cjs,graphql,html,json,env",
    "signal": "SIGTERM"
  }
}
