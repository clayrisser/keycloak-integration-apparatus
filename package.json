{
  "name": "keycloak-integration-apparatus",
  "version": "0.0.3",
  "description": "keycloak socket apparatus integration for kubernetes",
  "private": true,
  "sideEffects": false,
  "main": "dist/main.js",
  "source": "src/index.ts",
  "keywords": [
    "keycloak-socket"
  ],
  "homepage": "https://gitlab.com/risserlabs/community/keycloak-integration-apparatus",
  "bugs": {
    "url": "https://gitlab.com/risserlabs/community/keycloak-integration-apparatus/issues",
    "email": "email@clayrisser.com"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Clay Risser",
    "email": "email@clayrisser.com",
    "url": "https://clayrisser.com"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/risserlabs/community/keycloak-integration-apparatus"
  },
  "scripts": {
    "build": "make -s build",
    "clean": "make -s clean",
    "coverage": "make -s coverage",
    "format": "make -s format",
    "inc": "make -s inc",
    "lint": "make -s lint",
    "make": "make",
    "prepare": "make -s prepare",
    "purge": "make -s purge",
    "report": "make -s report",
    "spellcheck": "make -s spellcheck",
    "start": "make -s start",
    "test": "make -s test",
    "test:ui": "make test-ui",
    "test:watch": "make -s test-watch"
  },
  "dependencies": {
